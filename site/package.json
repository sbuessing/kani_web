{
  "name": "site",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startPublic": "ng serve --disable-host-check --host=0.0.0.0",
    "eStart": "ng serve --configuration emulators",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && tslint --project tsconfig.json",
    "prettier": "npx prettier --write \"**/*.{ts,html,scss}\"",
    "e2e": "ng e2e",
    "emulators": "npm --prefix functions build:watch | firebase emulators:start --only auth,storage,firestore,functions --export-on-exit=./emulator-data --import=./emulator-data",
    "updateStaging": "ng build --configuration production --aot && firebase hosting:channel:deploy staging --expires 30d",
    "updateProd": "ng build --configuration production --aot && firebase deploy --only hosting"
  },
  "engines": {
    "node": "16"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.1.1",
    "@angular/core": "^13.1.1",
    "@angular/elements": "^13.1.3",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "^13.1.1",
    "@angular/localize": "^13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.1",
    "@angular/router": "^13.1.1",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "bootstrap": "^5.1.3",
    "d3": "^7.2.1",
    "firebase": "^9.6.1",
    "firebaseui": "^6.0.0",
    "rxjs": "^7.5.1",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "^13.1.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "codelyzer": "^6.0.0",
    "eslint": "^8.2.0",
    "fuzzy": "^0.1.3",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4"
  }
}
